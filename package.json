    "concurrently": "^7.2.1"
  },
  "bugs": {
    "url": "https://github.com/BGrowcott/untitled-rat-project/issues"
  },
  "homepage": "https://github.com/BGrowcott/untitled-rat-project#readme",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "dotenv": "^16.0.1",
    "react-hook-form": "^7.31.3",
    "jwt-decode": "^3.1.2"
  }
